          <body name="imu_frame" pos="0 0 -0.11">
            <site name="imu_sensorsite" />
          </body>
        </body>
      </body>
    </body> -->

    <!-- MISC -->
    <light pos="0 0 1000" castshadow="true" />
  </worldbody>

  <!-- <equality>
    <weld body1="link6_mocap" body2="link6_dummy" class="tcp_compliance" relpose="0 0 0 0 0 0 0"/>
  </equality> -->

